<h2>All Results</h2>

<div class="row">
    <% if (results) { %>
        <% for (let i = 0; i < results.length; i++) { %>
            <div class="col card">
                <div class="card-body">
                <h5 class="card-title"><%=results[i].testName%></h5>

                <h6 class="card-subtitle mb-2 text-body-secondary"><%=results[i].taker%></h6>

                <a href="/tests/<%=results[i].testName%>/results?taker=<%=results[i].taker%>"" class="card-link">View Results</a>
                </div>
            </div>
        <% } %>
    <% } %>
</div>